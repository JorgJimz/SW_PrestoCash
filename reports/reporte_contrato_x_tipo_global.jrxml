	<parameter name="estado" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT p.des_pre AS 'prestamo', CONCAT(p,'-',id_con) ,DATE_FORMAT(fec_con,'%d-%b-%y') ,DATE_FORMAT(fec_ven_con,'%d-%b-%y'),
DATE_FORMAT(fec_rem_con,'%d-%b-%y'),cap_con,int_con,mon_con, des_est
FROM tb_contrato c
INNER JOIN tb_prestamo p ON c.tb_prestamo_cod_pre=p.cod_pre
INNER JOIN tb_estado_contrato ec ON c.tb_estado_contrato_id_est= ec.id_est
WHERE p.des_pre LIKE $P{tipos}
UNION
SELECT p.des_pre AS 'prestamo', CONCAT(p,'-',id_con),DATE_FORMAT(fec_con,'%d-%b-%y') ,DATE_FORMAT(fec_ven_con,'%d-%b-%y'),
DATE_FORMAT(fec_rem_con,'%d-%b-%y'),cap_con,int_con,mon_con, des_est
FROM tb_contrato_manual c
INNER JOIN tb_prestamo p ON c.tb_prestamo_cod_pre=p.cod_pre
INNER JOIN tb_estado_contrato ec ON c.tb_estado_contrato_id_est= ec.id_est
WHERE p.des_pre LIKE $P{tipos}
UNION
SELECT p.des_pre AS 'prestamo', CONCAT(p,'-',id_con),DATE_FORMAT(fec_con,'%d-%b-%y') ,DATE_FORMAT(fec_ven_con,'%d-%b-%y'),
DATE_FORMAT(fec_rem_con,'%d-%b-%y'),cap_con,int_con,mon_con, des_est
FROM tb_contrato_oro c
INNER JOIN tb_prestamo p ON c.tb_prestamo_cod_pre=p.cod_pre
INNER JOIN tb_estado_contrato ec ON c.tb_estado_contrato_id_est= ec.id_est
WHERE p.des_pre LIKE $P{tipos}]]>
	</queryString>
