	<parameter name="fecha" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT format(lc.ama_caja,2) AS 'amanece',
SUM(CASE WHEN tip_ing = 'S-REM.'
THEN oto_ing ELSE cap_ing+gan_ing+oto_ing END) as 'ingresos',
(SELECT format(SUM(case when tip_egr = 'EMP.($)' then mon_egr*(select ven_cam from tb_cambio where fec_cam=date_Format( $P{fecha},'%y-%m-%d')) else mon_egr end),2) FROM tb_egreso e
INNER JOIN tb_libro_caja lc
ON lc.id_caja = e.tb_libro_caja_id_caja
WHERE lc.fec_caja =$P{fecha}
)as 'egresos', format(lc.cer_caja,2) as 'cer_caja'
FROM tb_libro_caja lc
INNER JOIN tb_ingreso i
ON lc.id_caja = i.tb_libro_caja_id_caja
WHERE lc.fec_caja = $P{fecha}]]>
	</queryString>
