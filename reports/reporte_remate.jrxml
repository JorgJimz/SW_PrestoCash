	<parameter name="estado" class="java.lang.Integer"/>
	<parameter name="mes" class="java.lang.Integer"/>
	<parameter name="anio" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT CONCAT(c.p,'-',c.id_con) AS 'id_con', CONCAT(a.des_art,SPACE(1),a.mar_art, SPACE(1),a.mod_art) AS 'DESCRIPCION',dc.tas_det_con,DATE_ADD(c.fec_rem_con,INTERVAL 15 DAY)
FROM tb_contrato_manual c
INNER JOIN tb_detalle_contrato_manual dc
ON c.id_con=dc.tb_contrato_manual_id_con
INNER JOIN tb_articulo a
ON a.cod_art = dc.tb_articulo_cod_art
WHERE c.tb_estado_contrato_id_est = $P{estado}
AND MONTH(DATE_ADD(c.fec_rem_con,INTERVAL 15 DAY)) = $P{mes} AND YEAR(DATE_ADD(c.fec_rem_con,INTERVAL 15 DAY)) = $P{anio}
AND c.tb_prestamo_cod_pre <> 'C'
UNION
SELECT CONCAT(c.p,'-',c.id_con) AS 'id_con', CONCAT(a.des_art,SPACE(1),a.mar_art, SPACE(1),a.mod_art) AS 'DESCRIPCION',dc.tas_det_con,c.fec_rem_con
FROM tb_contrato c
INNER JOIN tb_detalle_contrato dc
ON c.id_con=dc.tb_contrato_id_con
INNER JOIN tb_articulo a
ON a.cod_art = dc.tb_articulo_cod_art
WHERE c.tb_estado_contrato_id_est = $P{estado}
AND MONTH(DATE_ADD(c.fec_rem_con,INTERVAL 15 DAY)) = $P{mes} AND YEAR(DATE_ADD(c.fec_rem_con,INTERVAL 15 DAY)) = $P{anio}
AND c.tb_prestamo_cod_pre <> 'C']]>
	</queryString>
