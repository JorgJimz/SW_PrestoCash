	<queryString language="SQL">
		<![CDATA[SELECT c.id_car,fec_car,CONCAT(p_con_asoc,'-',nro_con) as 'contrato',CONCAT(des_art,space(1),mar_art,space(1),mod_art) as 'descripcion', des_car FROM tb_cargo c
