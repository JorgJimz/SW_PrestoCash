	<queryString>
		<![CDATA[SELECT C.FLAG, C.NUMERO, C.CAPITAL, C.INTERES_MENSUAL, 
EC.DESCRIPCION AS ESTADO, 
A.DESCRIPCION AS ARTICULO, A.MARCA, A.MODELO,
DC.TASACION,
K.NOMBRES, K.PATERNO, K.MATERNO, K.DOCUMENTO, K.T_DOCUMENTO, K.TLF1, K.TLF2,
P.DESCRIPCION AS PRESTAMO
FROM contrato C 
	INNER JOIN detalle_contrato DC ON C.ID = DC.CONTRATO_ID 
	INNER JOIN articulo A ON DC.ARTICULO_ID = A.ID
	INNER JOIN cliente K ON C.CLIENTE_ID = K.ID
	INNER JOIN e_contrato EC ON C.E_CONTRATO_ID = EC.ID
	INNER JOIN prestamo P ON C.PRESTAMO_ID = P.ID
WHERE $X{IN,EC.DESCRIPCION,ESTADOS}]]>
	</queryString>
